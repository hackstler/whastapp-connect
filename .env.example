# URL del endpoint RAG al que se enviarán los mensajes (REQUERIDO)
INGEST_URL=https://rag-backbone.tu-dominio.com/whatsapp/ingest

# Ruta para guardar la sesión de WhatsApp (usar volumen persistente en producción)
SESSION_PATH=.wwebjs_auth

# TTL del cache LRU de deduplicación en milisegundos (default: 5 minutos)
DEDUP_TTL_MS=300000

# Número máximo de entradas en el cache LRU
DEDUP_MAX_SIZE=1000

# Puerto del servidor de health check
PORT=3000

# Nivel de log: debug | info | warn | error
LOG_LEVEL=info
